<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageId>TempFileStorage.SqlServer</PackageId>
    <Version>3.0.0</Version>
    <Authors>David Vercruyssen</Authors>
    <Company>Aranea</Company>
    <PackageProjectUrl>https://github.com/davidcruy/temp-file-storage</PackageProjectUrl>
    <RepositoryUrl>https://github.com/davidcruy/temp-file-storage.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <RootNamespace>TempFileStorage.SqlServer</RootNamespace>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="install.sql" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="install.sql" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="3.0.0" />
    <PackageReference Include="TempFileStorage" Version="3.0.0" />
  </ItemGroup>

</Project>
